<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
	<path>radar.ac</path>
 
	<animation>
		<type>scale</type>
		<x-offset>0.4</x-offset>
		<y-offset>1.0</y-offset>
		<z-offset>0.4</z-offset>
	</animation>

	<animation>
		<type>material</type>
		<object-name>radar</object-name>
	</animation>

	<animation>
		<type>select</type>
		<object-name>radar</object-name>
		<condition>
			<equals>
				<property>/sim/model/livery/name</property>
				<value>BS-AJ</value>
			</equals>
		</condition>
	</animation>

</PropertyList>
