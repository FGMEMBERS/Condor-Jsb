<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>inttourvitres</object-name>
        <object-name>throttleA</object-name>
    <object-name>throttleB</object-name>
    <object-name>throttleC</object-name>
    <object-name>throttleD</object-name>
    <object-name>mixtureA</object-name>
    <object-name>mixtureB</object-name>
    <object-name>mixtureC</object-name>
    <object-name>mixtureD</object-name>
    <object-name>propA</object-name>
    <object-name>propB</object-name>
    <object-name>propC</object-name>
    <object-name>propD</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>intvitres</object-name>
  </animation>


  <!-- Pilote -->
  <model>
    <path>Aircraft/Condor-Jsb/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -9.804 </x-m>
      <y-m> -0.470 </y-m>
      <z-m>  0.377 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>   0 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- Planche de bord -->
  <model>
    <path>Aircraft/Condor-Jsb/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -10.296 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>   0.208 </z-m>
    </offsets>
  </model>


  



</PropertyList>

